{
    "name": "softspring/mailer-bundle",
    "description": "This bundle provides some features that ease mailing for Symfony projects.",
    "license": "MIT",
    "type": "symfony-bundle",
    "require": {
        "php": "^7.0",
        "softspring/core-bundle": "^1.3.2",
        "symfony/mailer": "^4.4 || ^5.0"
    },
    "require-dev": {
        "symfony/console": "*",
        "symfony/framework-bundle": "*",
        "symfony/translation": "*",
        "symfony/yaml": "*",
        "twig/twig": "*"
    },
    "suggest": {
        "softspring/admin-bundle": "Use softspring admin templates",
        "twig/cssinliner-extra": "Use css inliner",
        "twig/inky-extra": "Use Inky mailing templates"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\MailerBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    }
}
