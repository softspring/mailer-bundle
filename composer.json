{
    "name": "softspring/mailer-bundle",
    "description": "This bundle provides some features that ease mailing for Symfony projects.",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.4",
        "softspring/admin-bundle": "^5.0",
        "softspring/core-bundle": "^5.0",
        "softspring/mime-translatable": "^5.0",
        "symfony/mailer": "^5.4|^6.0"
    },
    "require-dev": {
        "symfony/console": "^5.4|^6.0",
        "symfony/framework-bundle": "^5.4|^6.0",
        "symfony/translation": "^5.4|^6.0",
        "symfony/yaml": "^5.4|^6.0",
        "twig/twig": "*",
        "friendsofphp/php-cs-fixer": "9999999-dev"
    },
    "suggest": {
        "softspring/admin-bundle": "Use softspring admin templates",
        "twig/cssinliner-extra": "Use css inliner",
        "twig/inky-extra": "Use Inky mailing templates"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\MailerBundle\\": "src/"
        }
    },
    "extra": {
    }
}
